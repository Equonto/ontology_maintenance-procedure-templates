<http://spec.equonto.com/ontology/maintenance-procedure/conditional-maintenance-task-ontology>
        rdf:type             owl:Ontology ;
        owl:versionIRI       <http://spec.equonto.com/ontology/202312/maintenance-procedure/conditional-maintenance-task-ontology> ;
        iof-av:maturity      iof-av:Released ;
        rdfs:label           "Maintenance Procedures - Conditional Maintenance Task Ontology" ;
        owl:imports          <http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology> ;
        dcterms:creator      "The University of Western Australia & Equonto Pty. Ltd." ;
        dcterms:contributor  "Tim French: The University of Western Australia" ;
        dcterms:contributor  "Caitlin Woods: The University of Western Australia and Equonto" ;
        dcterms:contributor  "Melinda Hodkiewicz: The University of Western Australia" ;
        dcterms:license      "http://opensource.org/licenses/MIT" ;
        dcterms:title        "Ontology for Maintenance Procedure Documentation (OMPD) Conditional Maintenance Task Ontology" ;
        dcterms:publisher    "Equonto Pty. Ltd." ;
        dcterms:abstract     "The OMPD Conditional Mainteance Task Ontology captures conditial task information (i.e. in response to some limit or functional failure) present in some industrial maintenance procedures." .

lis:isAbout  rdf:type  owl:ObjectProperty .

_:b0    rdf:type            owl:Restriction ;
        owl:onProperty      lis:isAbout ;
        owl:someValuesFrom  proc:FunctionalFailure .

proc:FunctionalFailure
        rdf:type  owl:Class .

_:b1    rdf:type  owl:Class .

proc:FailureModeObservation
        rdf:type             owl:Class ;
        owl:equivalentClass  _:b1 .

_:b1    rdf:type            owl:Class ;
        owl:intersectionOf  _:b2 .

_:b3    rdf:rest   rdf:nil ;
        rdf:first  _:b0 .

_:b2    rdf:rest   _:b3 ;
        rdf:first  lis:InformationObject .

lis:hasParticipant  rdf:type  owl:ObjectProperty .

_:b4    rdf:type            owl:Restriction ;
        owl:onProperty      lis:hasParticipant ;
        owl:someValuesFrom  proc:MaintainableItem .

proc:MaintainableItem
        rdf:type  owl:Class .

proc:FunctionalFailure
        rdfs:subClassOf  _:b4 .

proc:addresses  rdf:type  owl:ObjectProperty .

_:b5    rdf:type            owl:Restriction ;
        owl:onProperty      proc:addresses ;
        owl:someValuesFrom  proc:FunctionalFailure .

proc:FunctionalFailure
        rdf:type  owl:Class .

_:b6    rdf:type  owl:Class .

proc:CorrectiveMaintenanceTask
        rdf:type             owl:Class ;
        owl:equivalentClass  _:b6 .

_:b6    rdf:type            owl:Class ;
        owl:intersectionOf  _:b7 .

_:b8    rdf:rest   rdf:nil ;
        rdf:first  _:b5 .

_:b7    rdf:rest   _:b8 ;
        rdf:first  proc:MaintenanceTask .

proc:resultsIn  rdf:type  owl:ObjectProperty .

_:b9    rdf:type            owl:Restriction ;
        owl:onProperty      proc:resultsIn ;
        owl:someValuesFrom  proc:FunctionalFailure .

proc:FunctionalFailure
        rdf:type  owl:Class .

proc:FailureEvent  rdfs:subClassOf  _:b9 .

proc:FunctionalFailure
        rdfs:subClassOf     lis:Activity ;
        rdf:type            owl:Class ;
        iof-av:maturity     iof-av:Released ;
        rdfs:comment        "An ISO: Activity that is represented in a OMPD: Failure Mode Observation and hasParticipant some OMPD: Maintainable Item" ;
        rdfs:label          "Functional Failure" ;
        iof-av:isPrimitive  "TRUE"^^xsd:boolean .

proc:FailureEvent  rdfs:subClassOf  lis:Event ;
        rdf:type            owl:Class ;
        iof-av:maturity     iof-av:Released ;
        rdfs:comment        "An ISO:Event that resuts in a OMPD: Functional Failure" ;
        rdfs:label          "Failure Event" ;
        iof-av:isPrimitive  "TRUE"^^xsd:boolean .

proc:FailureModeObservation
        rdfs:subClassOf     lis:InformationObject ;
        rdf:type            owl:Class ;
        iof-av:maturity     iof-av:Released ;
        rdfs:comment        "An ISO:Information object that denotes a OMPD:functional failure" ;
        rdfs:label          "Failure Mode Observation" ;
        iof-av:isPrimitive  "FALSE"^^xsd:boolean .

proc:CorrectiveMaintenanceTask
        rdfs:subClassOf     proc:MaintenanceTask ;
        rdf:type            owl:Class ;
        iof-av:maturity     iof-av:Released ;
        rdfs:comment        "A OMPD:maintenance task that addresses a OMPD:functional failure" ;
        rdfs:label          "Corrective Maintenance Task" ;
        iof-av:isPrimitive  "FALSE"^^xsd:boolean .

proc:resultsIn  rdf:type    owl:ObjectProperty ;
        rdfs:subPropertyOf  lis:before .

proc:triggers  rdf:type     owl:ObjectProperty ;
        rdfs:subPropertyOf  proc:directlyBefore .

proc:addresses  rdf:type    owl:ObjectProperty ;
        rdfs:subPropertyOf  lis:occursRelativeTo ;
        owl:inverseOf       proc:addressedBy .

proc:addressedBy  rdf:type  owl:ObjectProperty ;
        rdfs:subPropertyOf  lis:occursRelativeTo ;
        owl:inverseOf       proc:addresses .

proc:functionalFailureRealisedIn
        rdf:type                owl:ObjectProperty ;
        rdfs:subPropertyOf      lis:realizedIn ;
        rdf:type                owl:ObjectProperty ;
        owl:propertyChainAxiom  _:b10 .

_:b10   rdf:rest   _:b11 ;
        rdf:first  proc:functionalFailureRealizedIn .

_:b11   rdf:rest   rdf:nil ;
        rdf:first  proc:triggers .
