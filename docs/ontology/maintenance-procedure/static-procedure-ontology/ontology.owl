    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Component -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Component">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject"/>
        <rdfs:comment>A LIS:PhysicalObject that may have some OMPD:ResourceRole</rdfs:comment>
        <rdfs:label>Component</rdfs:label>
        <AnnotationVocabulary:example>a circuit board, a pump shaft, a resistor, a cable</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>A LIS:PhysicalObject that may have some OMPD:ResourceRole</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>an item of hardware, software or both that can be individually described and has a required function</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Hazard -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Hazard">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/RealizableEntity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#HazardRealizationProcess"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Disposition"/>
        <rdfs:comment>An LIS: Realizeable Entity that is realizrd in some OMPD: Hazard Realization Process</rdfs:comment>
        <rdfs:label>Hazard</rdfs:label>
        <AnnotationVocabulary:example>oil on the floor, a heavy object that needs to be moved, a high pressure pipe</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS: Realizeable Entity that is realizrd in some OMPD: Hazard Realization Process</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>source or situation with a potential to cause injury and ill health (ISO 45001:2018)</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#HazardInMaintenanceProcess -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#HazardInMaintenanceProcess">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Hazard"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#HazardRealizationProcess"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/occursRelativeTo"/>
                                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcess"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Disposition"/>
        <rdfs:comment>An OMPD: Hazard that is realized in some OMPD: Hazard Realization Process that occurs relative to some OMPD: Maintenance Process</rdfs:comment>
        <rdfs:label>HazardInMaintenanceProcess</rdfs:label>
        <AnnotationVocabulary:example>a confined space, working at heights, pressurised containers</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An OMPD: Hazard that is realized in some OMPD: Hazard Realization Process that occurs relative to some OMPD: Maintenance Process</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>source or situation with a potential to cause injury and ill health identified in a maintenance procedure (ISO 45001:2018)</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#HazardRealizationProcess -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#HazardRealizationProcess">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Activity"/>
        <rdfs:comment>An LIS: Activity that has the potential to cause harm</rdfs:comment>
        <rdfs:label>HazardRealizationProcess</rdfs:label>
        <AnnotationVocabulary:example>a slip on the oil, the lifting of the too-heavy object, the unconstrained release of pressure</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS: Activity that has the potential to cause harm</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>the process in which the hazard is realised and harm occurs</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#ItemInMaintenanceProcess -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#ItemInMaintenanceProcess">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject"/>
        <rdfs:comment>A LIS:PhysicalObject that partiticipates in some OMPD:MaintenanceProcess</rdfs:comment>
        <rdfs:label>ItemInMaintenanceProcess</rdfs:label>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>A LIS:PhysicalObject that partiticipates in some OMPD:MaintenanceProcess</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>a physical object that is involved in some part of a maintenance task</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintainableItem -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintainableItem">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/hasRole"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintainableItemRole"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject"/>
        <rdfs:comment>An LIS:PhysicalObject hat has a role OMPD:MaintainableItemRole</rdfs:comment>
        <rdfs:label>MaintainableItem</rdfs:label>
        <AnnotationVocabulary:adaptedFrom>https://spec.industrialontologies.org/iof/ontology/core/Core/MaintainableMaterialItem</AnnotationVocabulary:adaptedFrom>
        <AnnotationVocabulary:example>a mechanical seal, a motor, tyres on a car</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS:PhysicalObject hat has a role OMPD:MaintainableItemRole</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>an item (system or component) for which a maintenance strategy has been identified  (IOF Core Ontology)</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintainableItemRole -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintainableItemRole">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Role"/>
        <rdfs:comment>An LIS:Role that is the role of the item that work is performed on in a OMPD:MaintenanceProcess</rdfs:comment>
        <rdfs:label>MaintainableItemRole</rdfs:label>
        <AnnotationVocabulary:adaptedFrom>https://spec.industrialontologies.org/iof/ontology/core/Core/MaintainableMaterialItemRole</AnnotationVocabulary:adaptedFrom>
        <AnnotationVocabulary:example>a pump has the maintainable material item role when it is undergoing repair after a failure, a bearing has a maintainable material item role when it is been lubricated as part of a preventative maintenance program </AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS:Role that is the role of the item that work is performed on in a OMPD:MaintenanceProcess</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>role played by an item when there is a maintenance strategy describing how it is to be maintained (IOF Core Ontology)</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceAction -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceAction">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Activity"/>
        <rdfs:comment>An LIS:Activity that is a maintenance activity or a supporting activity involved in execution of a maintenance task</rdfs:comment>
        <rdfs:label>MaintenanceAction</rdfs:label>
        <AnnotationVocabulary:example>repair, replace, lubricate, clean, move</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS:Activity that is a maintenance activity or a supporting activity involved in execution of a maintenance task</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>a context-agnostic maintenance activity or supporting activity that is described within a maintenance task</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcedureDocument -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcedureDocument">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/isAbout"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcedureProcess"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject"/>
        <rdfs:comment>An LIS:InformationObject that is about some OMPD:MaineanceProcedureProcess</rdfs:comment>
        <rdfs:label>MaintenanceProcedureDocument</rdfs:label>
        <AnnotationVocabulary:example>a document describing how to change the oil in the car, or to replace the coupling on a pump</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS:InformationObject that is about some OMPD:MaineanceProcedureProcess</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>a digital or paper document describing a task or set of tasks for a maintainer to perform</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcedureProcess -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcedureProcess">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcess"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/hasActivityPart"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTask"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/representedBy"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcedureDocument"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcess"/>
        <rdfs:comment>An OMPD:MaintenanceProcess that is represented in some OMPD:MaintenanceProcedureDocument and has activity part some OMPD:MaintenanceTask</rdfs:comment>
        <rdfs:label>MaintenanceProcedureProcess</rdfs:label>
        <AnnotationVocabulary:example>engine replacement process</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An OMPD:MaintenanceProcess that is represented in some OMPD:MaintenanceProcedureDocument and has activity part some OMPD:MaintenanceTask</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>a process containing the steps described in the maintenance procedure as activity parts</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcess -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcess">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Activity"/>
        <rdfs:comment>An LIS: Activity, intended to repair or restore an item to a state in which it can perform its required function</rdfs:comment>
        <rdfs:label>MaintenanceProcess</rdfs:label>
        <AnnotationVocabulary:adaptedFrom>https://spec.industrialontologies.org/iof/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceProcess</AnnotationVocabulary:adaptedFrom>
        <AnnotationVocabulary:example>repair the laptop, replace the window frame, inspect the fluid level</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS: Activity, intended to repair or restore an item to a state in which it can perform its required function</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>the activities involved in restoring or retaining the function of an item (IOF Maintenance Reference Ontology)</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTask -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTask">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcess"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#representedBy"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTaskDescription"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#locallyEquivalentTo"/>
                        <owl:allValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTask"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#directActivityPartOf"/>
                        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                        <owl:onClass rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcedureProcess"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#directActivityPartOf"/>
                        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                        <owl:onClass rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTask"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#directlyBefore"/>
                        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                        <owl:onClass rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTask"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcess"/>
        <rdfs:comment>An OMPD:MaintenanceProcess that is: - a direct activity part of zero or one OMPD:MaintenanceProcedureProcess - an activity part of zero or more OMPD:MaintenanceProcedureProcess - a direct activity part of zero or one OMPD:MaintenanceTask - an activity part of zero or more OMPD:MaintenanceTask - occurs directly before zero or one Maintenance task - occurs before zero or more MaintenanceTask - occurs directly after zero or one MaintenanceTask - occurs after zero or more OMPD:MaintenanceTask</rdfs:comment>
        <rdfs:label>MaintenanceTask</rdfs:label>
        <AnnotationVocabulary:example>shut-off the valve, remove the bolts from the cover, replace the shaft</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An OMPD:MaintenanceProcess that is: - a direct activity part of zero or one OMPD:MaintenanceProcedureProcess - an activity part of zero or more OMPD:MaintenanceProcedureProcess - a direct activity part of zero or one OMPD:MaintenanceTask - an activity part of zero or more OMPD:MaintenanceTask - occurs directly before zero or one Maintenance task - occurs before zero or more MaintenanceTask - occurs directly after zero or one MaintenanceTask - occurs after zero or more OMPD:MaintenanceTask</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>a discrete activity within a multi-step maintenance process</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTaskDescription -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTaskDescription">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/isAbout"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTask"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject"/>
        <rdfs:comment>An LIS:InformationObject that is about some OMPD:MaintenanceTask</rdfs:comment>
        <rdfs:label>MaintenanceTaskDescription</rdfs:label>
        <AnnotationVocabulary:example>the section in a maintenance procedure description that describes an individual task in the process</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS:InformationObject that is about some OMPD:MaintenanceTask</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>the part of a document containing instructions for performing a maintenance task</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTaskDescriptionMedia -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTaskDescriptionMedia">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject"/>
        <rdfs:comment>An LIS:InformationObject has some proc:MediaType and is part of some OMPD:MaintenanceTaskDescription</rdfs:comment>
        <rdfs:label>MaintenanceTaskDescriptionMedia</rdfs:label>
        <AnnotationVocabulary:example>An image of the engine that is being worked on in a &quot;disassemble the engine&quot; maintenance task.</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS:InformationObject has some proc:MediaType and is part of some OMPD:MaintenanceTaskDescription</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>an descriptive information object of one media type that forms part of a maintenance task description</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Material -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Material">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject"/>
        <rdfs:comment>A LIS:PhysicalObject that may have some OMPD:ResourceRole</rdfs:comment>
        <rdfs:label>Material</rdfs:label>
        <AnnotationVocabulary:example>sheet metal, oil, lubricant</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>A LIS:PhysicalObject that may have some OMPD:ResourceRole</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>items (generally consumables) that are required for the maintenance procedure to be executed that cannot be individually described according to its primary function (generally consumables).</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MediaType -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MediaType">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject"/>
        <rdfs:comment>An LIS:InformationObject that is a type of media</rdfs:comment>
        <rdfs:label>MediaType</rdfs:label>
        <AnnotationVocabulary:example>image, text, video</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS:InformationObject that is a type of media</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>the format of a section of a maintenance procedure that describes an individual task in the process</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Permit -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Permit">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject"/>
        <rdfs:comment>A LIS:InformationObject that  may have some OMPD:ResourceRole</rdfs:comment>
        <rdfs:label>Permit</rdfs:label>
        <AnnotationVocabulary:example>confined space entry permit, hot work permit</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>A LIS:InformationObject that  may have some OMPD:ResourceRole</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>a document containing approval to progress a task</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Qualification -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Qualification">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Quality"/>
        <rdfs:comment>A LIS:Quality that is beared by zero or more OMPD:QualitifiedPerson</rdfs:comment>
        <rdfs:label>Qualification</rdfs:label>
        <AnnotationVocabulary:example>an electrical engineering qualification, a welding certificate</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>A LIS:Quality that is beared by zero or more OMPD:QualitifiedPerson</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>a capability of an individual to perform a specific task that has been assessed by a recognised 3rd party group</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#QualifiedPerson -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#QualifiedPerson">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/hasQuality"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Qualification"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#requirementOf"/>
                        <owl:someValuesFrom rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Activity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Person"/>
        <rdfs:comment>A LIS:Person that may have some OMPD:ResourceRole and has a OMPD:Qualification</rdfs:comment>
        <rdfs:label>QualifiedPerson</rdfs:label>
        <AnnotationVocabulary:example>a mechanical engineer, an electrician, a machinist</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>A LIS:Person that may have some OMPD:ResourceRole and has a OMPD:Qualification</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>the person recognised by a 3rd party group as having a specific capability</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Resource -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Resource">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/Object"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/hasRole"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#ResourceRole"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Object"/>
        <rdfs:comment>An LIS: Object that has some OMPD: Resource Role</rdfs:comment>
        <rdfs:label>Resource</rdfs:label>
        <AnnotationVocabulary:example>a crane, welding rods, a multi-meter, mechanic</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS: Object that has some OMPD: Resource Role</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>a tool, material, equipment or person required to complete a maintenance task</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#ResourceRole -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#ResourceRole">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/Role"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/roleOf"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/Object"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#requirementOf"/>
                                        <owl:someValuesFrom rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Activity"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Role"/>
        <rdfs:comment>An LIS:Role that is the role of an LIS:Object that is required to execute some LIS:Activity</rdfs:comment>
