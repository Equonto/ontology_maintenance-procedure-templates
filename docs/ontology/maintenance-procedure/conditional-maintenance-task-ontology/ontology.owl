    <!-- http://spec.equonto.org/ontology/maintenance-procedure/conditional-maintenance-task-ontology#CorrectiveMaintenanceTask -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/conditional-maintenance-task-ontology#CorrectiveMaintenanceTask">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTask"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/conditional-maintenance-task-ontology#addresses"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/conditional-maintenance-task-ontology#FunctionalFailure"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTask"/>
        <rdfs:comment>A OMPD:maintenance task that addresses a OMPD:functional failure</rdfs:comment>
        <rdfs:label>Corrective Maintenance Task</rdfs:label>
        <AnnotationVocabulary:example>Replace the leaking mechanical seal, replace the blown light bulb</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>A OMPD:maintenance task that addresses a OMPD:functional failure</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>maintenance carried out after fault recognition and intended to restore an item into a state in which it can perform a required function (EN13306)</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/conditional-maintenance-task-ontology#FailureEvent -->


    <owl:Class rdf:about="http://spec.equonto.org/ontology/maintenance-procedure/conditional-maintenance-task-ontology#FailureEvent">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/conditional-maintenance-task-ontology#resultsIn"/>
                <owl:someValuesFrom rdf:resource="http://spec.equonto.org/ontology/maintenance-procedure/conditional-maintenance-task-ontology#FunctionalFailure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An LIS:Event that resuts in a OMPD: Functional Failure</rdfs:comment>
        <rdfs:label>Failure Event</rdfs:label>
        <AnnotationVocabulary:adaptedFrom>https://spec.industrialontologies.org/iof/ontology/maintenance/MaintenanceReferenceOntology/FailureEvent</AnnotationVocabulary:adaptedFrom>
        <AnnotationVocabulary:example>explosion, seizure, loss of power, loss of control</AnnotationVocabulary:example>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
        <AnnotationVocabulary:semiFormalNaturalLanguageAxioms>An LIS:Event that resuts in a OMPD: Functional Failure</AnnotationVocabulary:semiFormalNaturalLanguageAxioms>
        <AnnotationVocabulary:subjectMatterExpertExplanation>event that causes an item to lose its ability to perform a required function (IOF Maintenance Reference Ontology)</AnnotationVocabulary:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- http://spec.equonto.org/ontology/maintenance-procedure/conditional-maintenance-task-ontology#FailureModeObservation -->


